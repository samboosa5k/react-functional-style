{
  "trailingComma": "es5",
  "tabWidth": 4,
  "useTabs": false,
  "printWidth": 80,
  "semi": true,
  "singleQuote": true,
  "bracketSpacing": true,
  "jsxBracketSameLine": true,
  "arrowParens": "always",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^react(.*)$",
    "^react",
    "^styled-components$",
    "^utils",
    "@types",
    "@modules",
    "@components",
    "^[./]",
    "^[.]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true,
  "importOrderGroupNamespaceSpecifiers": true,
  "importOrderParserPlugins": ["typescript", "jsx"]
}
